*SemanticSqueak-Core-accessing
sesqSortedCategorizedElements

	| elements nonGrataCategoryPrefixes |
	elements := self sesqCategorizedElements.
	
	nonGrataCategoryPrefixes := #('*etoys' '*60deprecated' '*61deprecated' '*autogenerated').
	elements := (elements associations reject: [:assoc | assoc key asLowercase beginsWithAnyOf: nonGrataCategoryPrefixes]) as: elements species.
	
	^ elements
		withKeysSorted:
			[:category | (category beginsWith: '*') asBit] ascending
				, [:category | category asLowercase] ascending